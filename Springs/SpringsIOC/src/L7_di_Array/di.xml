    <!DOCTYPE beans PUBLIC "-//SPRING//DTD BEAN 2.0//EN"
			"http://www.springframework.org/dtd/spring-beans-2.0.dtd">

<beans>
    <bean id="e" class="L7_di_Array.engine">
        <property name="model" value="2018"/>
        
    </bean>
    <bean id="e1" class="L7_di_Array.engine">
        <property name="model" value="2017"/>
        
    </bean>
    <bean id="e2" class="L7_di_Array.engine">
        <property name="model" value="2016"/>
        
    </bean>
    <bean id="T" class="L7_di_Array.car">
        <property name="name" >
            <list>
                <value>Audi</value>
                <value>lamborgini</value>
                <value>maruti 800</value>
            </list>
        </property>
        <property name="engine" >
            <list>
                <ref bean="e"/>
                <ref bean="e1"/>
                <ref bean="e1"/>   <!--Repetation possible as this is Array(list) -->
            </list>
        </property>
    </bean>
</beans>

